  "devDependencies": {
    "airtap": "^4.0.4",
    "airtap-playwright": "^1.0.1",
    "airtap-sauce": "^1.1.0",
    "dependency-check": "^4.1.0",
    "hallmark": "^4.1.0",
    "nyc": "^15.0.0",
    "standard": "^16.0.4",
    "tape": "^5.5.2",
    "uuid": "^3.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Level/browser-level.git"
